{
  "name": "jagadeshanh/logaudit",
  "description": "Logaudit for laravel 5.4 and up",
  "license": "MIT",
  "keywords": [
    "logaudit",
    "laravel",
    "jagadeshanh",
    "laravel email exceptions"
  ],
  "homepage": "https://github.com/jagadeshanh/logaudit",
  "type": "library",
  "authors": [
    {
      "name": "Jagadesha NH",
      "email": "jagadeshanh@gmail.com",
      "homepage": "https://creativewebdesigners.in",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "~5.4.0|~5.5.0|~5.6.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.6.0",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "Jagadeshanh\\Logaudit\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jagadeshanh\\Logaudit\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jagadeshanh\\Logaudit\\ServiceProvider"
      ]
    }
  }
}
